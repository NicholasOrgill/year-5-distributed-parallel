public class Bakery implements Lock {
    
    boolean[] flag;
    Label[] label;

    public Bakery(int n) {
        flag = new boolean[n];
        label = new Label[n];
        for (int i = 0; i < n; i++) {
            flag[i] = false;
            label[i] = 0;
        }
    }

    public void lock() {
        flag[i] = true;
    }

}